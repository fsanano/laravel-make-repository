{
    "name": "fsanano/laravel-make-repository",
    "description": "A simple fork to create a make:repository command for Laravel 5+",
    "type": "library",
    "require": {
        "php": ">=5.4.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Fokin",
            "email": "memonanofsa@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "FsaNano\\LaravelMakeRepository\\RepositoryServiceProvider"
            ]
        }
    },
    
    "autoload": {
        "psr-4": {
            "FsaNano\\LaravelMakeRepository\\": "src/"
        },
        "classmap": ["src/"]
    }
}
